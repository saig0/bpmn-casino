    <decisionTable id="DecisionTable_1byjhiv" hitPolicy="FIRST">
      <input id="Input_1" label="Hand cards">
        <inputExpression id="InputExpression_1" typeRef="Any">
          <text>hand_cards.type</text>
        </inputExpression>
      </input>
      <input id="InputClause_1x0ip14" label="Remaining rounds">
        <inputExpression id="LiteralExpression_0gc38re" typeRef="number">
          <text>round down(deck_size / player_count, 0)</text>
        </inputExpression>
      </input>
      <input id="InputClause_0cy8mxp" label="Has defuse card">
        <inputExpression id="LiteralExpression_0xlhgry" typeRef="boolean">
          <text>list contains(hand_cards.type, "DEFUSE") </text>
        </inputExpression>
      </input>
      <input id="InputClause_0ms7dyz" label="Remaining player turns">
